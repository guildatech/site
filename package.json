{
  "name": "guildatech-site",
  "scripts": {
    "dev": "next",
    "build": "npm run prettier && npm run lint && next build",
    "start": "next start",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "prettier": "prettier --single-quote --trailing-comma all --write 'pages/**/*.js' 'components/**/*.js' 'services/**/*.js'",
    "lint": "eslint --fix --ext .js pages components services",
    "lint:watch": "esw -w --fix services"
  },
  "dependencies": {
    "ckeditor4-react": "^1.1.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "next": "^8.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.2",
    "react-scripts": "^3.4.1"
  },
  "licence": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && eslint"
    }
  },
  "proxy": "http://api.teste.com.br/v1",
  "devDependencies": {
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^3.1.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "qs": "^6.9.1"
  }
}
